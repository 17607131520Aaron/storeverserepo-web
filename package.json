{
  "name": "storeverserepo-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode test",
    "dev:test": "vite --mode test",
    "dev:dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build": "tsc -b && vite build --mode test",
    "build:test": "tsc -b && vite build --mode test",
    "build:dev": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,less,md,html,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,less,md,html,yml,yaml}\"",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,less}\" --fix",
    "check": "pnpm lint && pnpm format:check && pnpm stylelint",
    "fix": "pnpm lint:fix && pnpm format && pnpm stylelint:fix",
    "preview": "vite preview",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop storeverserepo-web",
    "pm2:restart": "pm2 restart storeverserepo-web",
    "pm2:delete": "pm2 delete storeverserepo-web",
    "pm2:logs": "pm2 logs storeverserepo-web",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "pm2:unstartup": "pm2 unstartup",
    "pm2:deploy": "pnpm build:prod && pm2 restart storeverserepo-web",
    "prepare": "husky",
    "log-server": "node scripts/log-ws-server.js",
    "log-server:dev": "node scripts/log-ws-server.js --port 8082"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@uidotdev/usehooks": "^2.4.1",
    "ahooks": "^3.9.6",
    "antd": "^6.1.1",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "dexie": "^4.2.1",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "echarts-gl": "^2.0.9",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "postcss-split": "^0.0.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-barcode": "^1.6.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.11.0",
    "react-virtualized": "^9.22.6",
    "recharts": "^3.6.0",
    "sass": "^1.97.0",
    "semver": "^7.7.3",
    "socket.io-client": "^4.8.1",
    "three": "^0.169.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.169.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lightningcss": "^1.30.2",
    "lint-staged": "^16.2.7",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.13.0",
    "terser": "^5.44.1",
